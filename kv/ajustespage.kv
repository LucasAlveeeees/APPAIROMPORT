<AjustesPage>:

    FloatLayout:



        Label:
            canvas.before:
                Rectangle:
                    pos: self.pos
                    size:self.size
                    source:"icones/botao.png"

            pos_hint:{"right": 0.85, "top": 0.95}
            size_hint: 0.7, 0.2
            bold: True
            text: "Seu Codigo de Cliente:1"
            id: id_cliente
        ImageButton:
            pos_hint: {"right": 0.5, "top": 0.7}
            size_hint: 0.5, 0.15
            source: "icones/clientes1.jpg"
            on_release:app.mudar_tela("mudarfotoperfilpage")
        LabelButton:
            pos_hint: {"right": 0.9, "top": 0.7}
            size_hint: 0.5, 0.15
            text: "Mudar Foto de Perfil"
        ImageButton:
            pos_hint: {"right": 0.5, "top": 0.5}
            size_hint: 0.5, 0.15
            source: "icones/clientes1.jpg"
            on_release:app.mudar_tela("novaospage")
        LabelButton:
            pos_hint: {"right": 0.9, "top": 0.5}
            size_hint: 0.5, 0.15
            text: "Novo Cliente"
        ImageButton:
            pos_hint: {"right": 0.5, "top": 0.3}
            size_hint: 0.5, 0.15
            source: "icones/clientes1.jpg"
            on_release:app.mudar_tela("acompanharospage")
        LabelButton:
            pos_hint: {"right": 0.9, "top": 0.3}
            size_hint: 0.5, 0.15
            text: "Acompanhar Garantias"
        FloatLayout:
            pos_hint: {"right": 1, "top": 0.12}
            size_hint: 1, 0.12
            canvas:
                Color:
                    rgb:utils.get_color_from_hex("#006D7E")
                Rectangle:
                    size: self.size
                    pos: self.pos
            ImageButton:
                pos_hint: {"right": 0.2, "top": 0.9}
                size_hint: 0.2,0.8
                source:"icones/voltar.png"
                on_release: app.mudar_tela("homepage")





